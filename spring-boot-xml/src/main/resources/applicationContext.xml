<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="alto" class="com.springboot.beans.Alto" />
	<bean name="swift" class="com.springboot.beans.Swift" />
	<bean name="employee1"
		class="com.springboot.beans.Employee">
		<property name="name" value="Ramjeet"></property>
		<property name="car" ref="alto"></property>
	</bean>
</beans>